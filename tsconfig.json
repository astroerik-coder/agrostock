{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "baseUrl": ".",
    "paths": {
      "@types/*": ["app/types/*"],
      "@data/*": ["src/data/*"],
      "@assets/*": ["assets/*"]
    }
}
